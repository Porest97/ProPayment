@model IEnumerable<ProPayment.Models.MatchFee>

@{
    ViewData["Title"] = "Index";
}

<h1>Matcharvoden Region Öst 2019-2020</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MatchFeeString)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HDFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LDFee)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MatchFeeString)
            <td>
                @Html.DisplayFor(modelItem => item.HDFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LDFee)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
